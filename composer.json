{
  "name": "istianxin/decorator-laravel",
  "description": "Decorate your method/function in laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tian Xin",
      "email": "upc_tianxin@foxmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.0",
    "laravel/framework": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "squizlabs/php_codesniffer": "^3.5",
    "ext-xdebug": "*",
    "orchestra/testbench": "v4.5.0"
  },
  "autoload": {
    "psr-4": {
      "IsTianXin\\Decorator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IsTianXin\\Decorator\\Tests\\": "tests"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php -r \"file_exists('.git') && copy('.git-pre-commit', '.git/hooks/pre-commit') && chmod('.git/hooks/pre-commit', 0755);\""
    ]
  }
}
